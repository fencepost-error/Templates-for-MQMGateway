modmqttd:
  converter_search_path:
    - /usr/lib/modmqttd
  converter_plugins:
    - stdconv.so
    - exprconv.so

modbus:
  networks:
    - name: annex
      address: outdoors.modbus.lan
      port: 502
    - name: network
      address: indoors.modbus.lan
      port: 502
    - name: switchboard
      address: basement.modbus.lan
      port: 502

mqtt:
  client_id: modbus
  refresh: 30s
  broker:
    host: localhost
  objects:
